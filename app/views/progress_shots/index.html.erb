<p id="notice"><%= notice %></p>

<h1>Progress Shots</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @progress_shots.each do |progress_shot| %>
      <tr>
        <td><%= progress_shot.title %></td>
        <td><%= progress_shot.description %></td>
        <td><%= progress_shot.project %></td>
        <td><%= link_to 'Show', progress_shot %></td>
        <td><%= link_to 'Edit', edit_progress_shot_path(progress_shot) %></td>
        <td><%= link_to 'Destroy', progress_shot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Progress Shot', new_progress_shot_path %>
